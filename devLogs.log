[build up webRTC env]
<1> install depot_tool
mkdir -p /md/proj/webRTC
cd /md/proj/webRTC
git clone https://chromium.googlesource.com/chromium/tools/depot_tools.git --config http.proxy=192.168.179.1:8123
vim ~/.bashrc:
export PATH=/md/proj/webRTC/depot_tools:$PATH
<2> config gsutil proxy env
//follow:
//(1) https://cloud.google.com/storage/docs/gsutil/commands/config
//(2) https://groups.google.com/a/chromium.org/forum/#!msg/chromium-packagers/0eE3oC3lBFw/Ykg1ueIKAQAJ
vim ~/.boto:
[Boto]
  proxy=192.168.179.1
  proxy_port=1080
<3> export proxy env
export http_proxy=socks5://192.168.179.1:1080
export https_proxy=socks5://192.168.179.1:1080
export sock5_proxy=socks5://192.168.179.1:1080
export NO_AUTH_BOTO_CONFIG=~/.boto 
<4> fetch code
mkdir webrtc-checkout
cd webrtc-checkout
fetch --nohooks webrtc
gclient sync
<5> create local branch
cd src
git reset --hard HEAD
./build/install-build-deps.sh --no-chromeos-fonts
gclient sync


